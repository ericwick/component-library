{
  "env": {
    "commonjs": false,
    "es6": true,
    "node": false
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-console": "off",
    "eqeqeq": 1,
    "no-unused-vars": "off",
    "no-undef": "off",
    "array-bracket-spacing": 1,
    "no-async-promise-executor": "off",
    "no-useless-catch": "off",
    "no-misleading-character-class": "off"
  }
}
